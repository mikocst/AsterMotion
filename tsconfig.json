{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components": ["src/components/index.ts"],
      "@components/*": ["src/components/*"],
      "@types": ["src/types/index.ts"],
      "@types/*": ["src/types/*"],
      "@assets/": ["assets/*"] 
    }
  }
}